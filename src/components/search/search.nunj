{% from "../icon/icon.nunj" import icon %}

{% macro search(className) %}
	<a href="#" class="search {% if className %}{{ className }}{% endif %}" aria-label="Search">
		{{ icon({
				iconName: 'search',
				className: 'icon',
				attributes: 'width="24" height="24"'
				})
		}}
	</a>
{% endmacro %}
